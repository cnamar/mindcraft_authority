        app:layout_constraintTop_toBottomOf="@+id/classes" />

    <TextView
        android:id="@+id/number_plate"
        android:layout_width="145dp"
        android:layout_height="73dp"
        android:layout_marginStart="147dp"
        android:layout_marginLeft="147dp"
